{
  "name": "@daniel-mall/kv-ui",
  "version": "0.1.0",
  "description": "Daniel Mall Vue UI 组件库",
  "type": "module",
  "main": "./dist/kv-ui.umd.cjs",
  "module": "./dist/kv-ui.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/kv-ui.js",
      "require": "./dist/kv-ui.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
    "dev": "vite build --watch",
    "lint": "eslint src",
    "typecheck": "vue-tsc --noEmit"
  },
  "keywords": [
    "vue",
    "ui",
    "components",
    "daniel-mall"
  ],
  "author": "Daniel Mall Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RyenToretto/daniel-mall.git",
    "directory": "kv-ui"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "sass": "^1.70.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-dts": "^3.7.1",
    "vue": "^3.4.15",
    "vue-tsc": "^1.8.27"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
